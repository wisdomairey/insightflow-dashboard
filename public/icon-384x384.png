<!-- Simple SVG icon for PWA -->
<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="512" height="512" fill="#3b82f6" rx="64"/>
  
  <!-- Chart icon -->
  <g fill="white">
    <!-- Bar chart -->
    <rect x="100" y="300" width="40" height="120" rx="4"/>
    <rect x="160" y="250" width="40" height="170" rx="4"/>
    <rect x="220" y="200" width="40" height="220" rx="4"/>
    <rect x="280" y="180" width="40" height="240" rx="4"/>
    <rect x="340" y="220" width="40" height="200" rx="4"/>
    
    <!-- Line with dots -->
    <circle cx="120" cy="160" r="8"/>
    <circle cx="180" cy="140" r="8"/>
    <circle cx="240" cy="120" r="8"/>
    <circle cx="300" cy="100" r="8"/>
    <circle cx="360" cy="110" r="8"/>
    
    <!-- Lines connecting dots -->
    <path d="M120 160 L180 140 L240 120 L300 100 L360 110" stroke="white" stroke-width="4" fill="none"/>
  </g>
  
  <!-- App name -->
  <text x="256" y="80" text-anchor="middle" font-family="Arial, sans-serif" font-size="48" font-weight="bold" fill="white">IF</text>
</svg>
